    "i18next": "^25.4.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "openapi-fetch": "^0.14.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.49.2",
    "react-i18next": "^15.7.0",
    "single-spa": "^6.0.3",
    "single-spa-react": "^6.0.1",
    "styled-components": "^6.1.8",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "browserslist": {
    "production": [
      ">0.2%",
