{
  "name": "@orgst/member-card",
  "version": "1.0.0",
  "private": true,
  "description": "Microfrontend - Cracha digital dos voluntarios da comunidade Orgst",
  "main": "dist/orgst-member-card.js",
  "type": "module",
  "scripts": {
    "dev": "webpack serve --port 9001",
    "dev:standalone": "webpack serve --env standalone --port 9001",
    "build": "concurrently npm:build:*",
    "build:webpack": "webpack --mode=production",
    "analyze": "webpack --mode=production --env analyze",
    "type-check": "tsc --noEmit",
    "format": "prettier --write . --ignore-path ../../.prettierignore",
    "check-format": "prettier --check . --ignore-path ../../.prettierignore",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "cross-env BABEL_ENV=test jest --maxWorkers=50%",
    "test:watch": "cross-env BABEL_ENV=test jest --watch --maxWorkers=50%",
    "test:coverage": "cross-env BABEL_ENV=test jest --coverage --maxWorkers=50%",
    "test:ci": "cross-env BABEL_ENV=test jest --coverage --watchAll=false --maxWorkers=50%"
  },
  "devDependencies": {
    "@orgst/config": "workspace:*",
    "@orgst/eslint-config": "workspace:*",
    "@babel/core": "^7.23.7",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/plugin-transform-runtime": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-react": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@babel/runtime": "^7.23.8",
    "@happy-dom/jest-environment": "^20.5.0",
    "@jest/globals": "^29.7.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.5",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@types/styled-components": "^5.1.34",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-styled-components": "^2.1.4",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.34.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "prettier": "^3.6.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-config-single-spa-react-ts": "^4.0.0",
    "webpack-dev-server": "^5.2.2",
    "webpack-merge": "^5.10.0"
